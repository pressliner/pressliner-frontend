{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier", "tslint-react-hooks"],
  "rules": {
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "no-submodule-imports": false,
    "ordered-imports": false,
    "no-namespace": false,
    "max-line-length": [true, 120],
    "no-empty-interface": false,
    "jsx-no-lambda": false,
    "no-implicit-dependencies": [true, ["@components", "@styles"]]
  }
}
